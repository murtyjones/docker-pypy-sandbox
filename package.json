{
  "name": "docker-pypy-sandbox",
  "version": "0.0.11",
  "description": "A docker-based sandbox to execute untrusted python code code",
  "main": "./lib/Sandbox",
  "scripts": {
    "test": "./scripts/run_tests.sh",
    "test-unit": "mocha test/unit/** --require babel-core/register",
    "start": "node app.js",
    "clean": "docker stop $(docker ps -q) && rm -rf tmp",
    "cleanAndRun": "docker stop $(docker ps -q) && rm -rf tmp && node --harmony_destructuring app.js",
    "debug": "node debug app.js",
    "docker-build": "docker build -t pypy-sandbox container/.",
    "docker-stop-all": "docker stop $(docker ps -q)",
    "docker-run": "docker run -it --rm -p 3000:3000 pypy-sandbox",
    "docker-local": "npm run docker-build && npm run docker-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/murtyjones/docker-pypy-sandbox.git"
  },
  "author": "Christophe Tafani-Dereeper <christophe@tafani-dereeper.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/murtyjones/docker-pypy-sandbox/issues"
  },
  "homepage": "https://github.com/murtyjones/docker-pypy-sandbox#readme",
  "dependencies": {
    "async": "^2.0.0-rc.3",
    "dockerode": "^2.2.10",
    "fs-extra": "^0.26.7",
    "lodash": "^4.8.2",
    "request": "^2.70.0",
    "uuid": "^3.0.0",
    "winston": "^2.2.0",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "proxyquire": "^1.8.0",
    "jasmine-node": "^1.14.5",
    "sinon": "^4.1.2"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "keywords": [
    "docker",
    "python",
    "pypy",
    "sandbox"
  ]
}
